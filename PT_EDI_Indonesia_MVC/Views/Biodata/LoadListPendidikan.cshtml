@model List<PendidikanTerakhir>

<h4>Pendidikan Terakhir</h4>
@if (Model is not null)
{
    foreach (var pendidikan in Model)
    {
        <form method="post" class="row" enctype="multipart/form-data">
            <input asp-for="@pendidikan.Id" hidden />
            <input asp-for="@pendidikan.BiodataId" hidden />

            <div class="row">
                <div class="col-10">
                    <div class="border p-3">
                        @*<div asp-validation-summary="ModelOnly"></div>*@
                        <div class="form-floating py-2 col-12">
                            <input asp-for="@pendidikan.JenjangPendidikanTerakhir" class="form-control border-0 shadow" />
                            <label asp-for="@pendidikan.JenjangPendidikanTerakhir" class="ms-2"></label>
                            <span asp-validation-for="@pendidikan.JenjangPendidikanTerakhir" class="text-danger"></span>
                        </div>
                    </div>
                </div>
            </div>
        </form>

    }

}
else
{
    <p>No Pendidikan Terakhir Found</p>
}